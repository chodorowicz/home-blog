    "autoprefixer": "^10.4.4",
    "gray-matter": "^4.0.3",
    "image-size": "1.0.1",
    "next": "12.1.0",
    "next-mdx-remote": "^4.0.0",
    "next-seo": "5.1.0",
    "next-themes": "^0.1.1",
    "postcss": "^8.4.12",
    "preact": "^10.6.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reading-time": "1.5.0",
    "rehype-katex": "^6.0.2",
    "remark-autolink-headings": "7.0.1",
    "remark-code-titles": "0.1.2",
